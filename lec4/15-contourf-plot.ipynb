   "source": [
    "import numpy as np\n",
    "from matplotlib import pyplot as plt\n",
    "\n",
    "x = np.linspace(-2, 2, 100)\n",
    "y = np.linspace(-2, 2, 100)\n",
    "X, Y = np.meshgrid(x, y)\n",
    "Z = np.exp(-X**2 - Y**2)\n",
    "plt.contourf(X, Y, Z, cmap='viridis')\n",
    "plt.colorbar()\n",
    "plt.title('Filled Contour Plot')\n",
    "plt.show()"
   ]
  }
 ],
